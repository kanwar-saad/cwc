		port = mPort;
		dataSock = new UDPConnection("", ConfigData.getBSAddress(), ConfigData.getDataPort(), ConfigData.getBSPort(), false);
	}
	
	public void run(){
		
	}
	
	
	
	
	private boolean rx_data(){
		
		return true;
	}
	
}
